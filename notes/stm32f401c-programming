1. sudo xbps-install -S cross-arm-none-eabi base-devel stlink cross-arm-none-eabi-gdb
2.
git clone https://github.com/STMicroelectronics/stm32f4xx-hal-driver
git clone https://github.com/STMicroelectronics/STM32CubeF4
git clone https://github.com/STMicroelectronics/cmsis-device-f4/
git clone https://github.com/STM32-base/STM32-base
git clone https://github.com/libopencm3/libopencm3
3. 
openocd -f board/stm32f4discovery.cfg -c "telnet_port 4445; tcl_port 6667; gdb_port 3334"
